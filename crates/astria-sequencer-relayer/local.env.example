# Log level. One of debug, info, warn, or error
ASTRIA_COMPOSER_LOG="astria_composer=info"

# Address of the API server
ASTRIA_COMPOSER_API_LISTEN_ADDR="0.0.0.0:0"

# Address of the RPC server for the sequencer chain
ASTRIA_COMPOSER_SEQUENCER_URL="http://127.0.0.1:26657"

# A list of execution <chain_id_1>::<rpc_server_1>,<chain_id_2>::<rpc_server_2>.
# Chain IDs are not case sensitive. If an ID is repeated, the last list item is used.
ASTRIA_COMPOSER_ROLLUPS="astriachain::ws://127.0.0.1:8545"

# Private key for the sequencer account used for signing transactions
# Must be a hex-encoded 32-byte array (64-character hex string)
ASTRIA_COMPOSER_PRIVATE_KEY="2bd806c97f0e00af1a1fc3328fa763a9269723c8db8fac4f93af71db186d6e90"

# Address of sequencer/cometbft/tendermint to request new blocks.
ASTRIA_SEQUENCER_RELAYER_SEQUENCER_ENDPOINT="http://127.0.0.1:26657"

# Address of the celestia node
ASTRIA_SEQUENCER_RELAYER_CELESTIA_ENDPOINT="http://127.0.0.1:26658"

# The bearer token to submit sequencer blocks as blobs to Celestia.
ASTRIA_SEQUENCER_RELAYER_CELESTIA_BEARER_TOKEN="<JWT Bearer token>"

# The gas limit when paying for submitting blobs to dcelestia
ASTRIA_SEQUENCER_RELAYER_GAS_LIMIT=1000000

# Whether to disable writing to celestia
ASTRIA_SEQUENCER_RELAYER_DISABLE_WRITING=false

# The duration in milliseconds that sequencer-relayer sleeps before requesting a new block from sequencer.
ASTRIA_SEQUENCER_RELAYER_BLOCK_TIME=1000

# The path to the file storing the cometbft validator key. Used to sign data before submitting it to celestia.y.json";
ASTRIA_SEQUENCER_RELAYER_VALIDATOR_KEY_FILE=.cometbft/config/priv_validator_key.json
ASTRIA_SEQUENCER_RELAYER_RPC_PORT: u16,
ASTRIA_SEQUENCER_RELAYER_P2P_PORT: u16,
ASTRIA_SEQUENCER_RELAYER_BOOTNODES: Option<Vec<String>>,
ASTRIA_SEQUENCER_RELAYER_LIBP2P_PRIVATE_KEY: Option<String>,
ASTRIA_SEQUENCER_RELAYER_LOG: String,
